*{margin:0px;padding:0px;}
a{
    text-decoration: none!important;
}
.container{
    width: 1190px!important;
}
.container-fluid{
    padding: 0px;
}

#logotop{
    height:20px;
    background: #f5f5f5;
    min-width: 1190px;
    border-bottom: 1px solid #ededed;
    font-size: 12px;
}
@media (min-width: 1px){
 .nav-justified>li {
        display: table-cell;
        width: 1%;
    }
    .nav-justified > li > a {
        margin-bottom: 0;
    }
}
#logobg{
    min-width: 1190px;
    height: 80px;
    background: url(../../images/logo_bg.jpg) no-repeat;
    background-size: 100% 100%;
}

#logo{
    height: 80px;

}
#logo .col-xs-1 img{
    vertical-align: top;
    margin-top: 11px;
}
#logo .col-xs-6{
    line-height: 74px;
    padding-left: 0px;
}
#logo .col-xs-6 span{
    font-size: 30px;
    color: #169ccc;
    font-weight: bold;

}
