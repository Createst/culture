    background-color: white;
    color: #428BCA;
}

#nav li a{
    border-radius: 0px;
    font-size: 16px;
    font-weight: bold;

}

#nva-bg{
    min-width: 1190px;
    background-color: #428BCA;
}
#header-nav{
    padding: 0px;

